{
  "name": "ts-templete",
  "version": "1.0.0",
  "description": "TypeScript Template",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node src/main.ts",
    "start": "node dist/main.js",
    "build": "rm -rf dist && tsc",
    "check": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write --list-different",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "=5.4.3"
  }
}
